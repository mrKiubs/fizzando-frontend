    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/masonry-layout": "^4.2.8",
    "@types/node": "^18.18.0",
    "concurrently": "^9.2.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.1.10",
    "typescript": "~5.6.2"
  }
}
