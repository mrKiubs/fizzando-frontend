{
  "name": "cocktail-app",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng build --configuration production",
    "serve:ssr": "node dist/cocktail-app/server/server.mjs",
    "dev:ssr": "concurrently -k \"ng build --configuration development --watch\" \"nodemon --watch dist/cocktail-app/server --ext mjs --exec node dist/cocktail-app/server/server.mjs\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.2.18",
    "@angular/common": "^19.2.14",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.2.14",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.2.18",
    "@angular/platform-browser": "^19.2.14",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/platform-server": "^19.2.14",
    "@angular/router": "^19.0.0",
    "@angular/ssr": "^19.2.14",
    "express": "^4.21.2",
    "imagesloaded": "^5.0.0",
    "masonry-layout": "^4.2.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.7",
    "@angular/cli": "^19.0.7",
    "@angular/compiler-cli": "^19.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/masonry-layout": "^4.2.8",
    "@types/node": "^18.18.0",
    "concurrently": "^9.2.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.1.10",
    "typescript": "~5.6.2"
  },
  "engines": {
    "node": "20.x"
  }
}
